<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="cs205.a3.song.FirstFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/activity_main">
        <action
            android:id="@+id/action_FirstFragment_to_fullscreenFragment"
            app:destination="@id/fullscreenFragment" />
    </fragment>
    <fragment
        android:id="@+id/fullscreenFragment"
        android:name="cs205.a3.FullscreenFragment"
        android:label="fragment_fullscreen"
        tools:layout="@layout/game_fullscreen" />
</navigation>